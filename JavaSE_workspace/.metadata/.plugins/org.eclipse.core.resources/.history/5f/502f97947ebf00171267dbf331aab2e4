package cn.laoliu_05;

public class StringTest {
	public static void main(String[] args) {
		String s1 = "";
		int[] arr={1,2,3};
		
		s1+="[";
		
		for(int i=0;i<arr.length;i++){
			s1 += arr[i];
			if(i==arr.length -1){				
				s1+=']';
			}else{
				s1+=',';
			}
		}
		
		System.out.println(s1);
		
	}
}
