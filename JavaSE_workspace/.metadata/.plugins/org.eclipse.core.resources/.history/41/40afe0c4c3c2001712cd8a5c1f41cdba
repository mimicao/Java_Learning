package cn.laoliu_03;

public class RegexSplitDemo {
	public static void main(String[] args) {
		String s = "helloaaaWorld";
		String regex = "a{2}";
		String regex = "a{2}";
		String regex = "a{2}";

		String[] sp = s.split(regex);

		for (int i = 0; i < sp.length; i++) {
			System.out.println(sp[i]);
		}
	}
}
