		Collection<Object> c3 = new ArrayList<Dog>(); 
		Collection<Object> c4 = new ArrayList<Cat>();
		
		Collection<Object> c1 = new ArrayList<Object>();
		Collection<Object> c2 = new ArrayList<Animal>(); 
		Collection<Object> c3 = new ArrayList<Dog>(); 
