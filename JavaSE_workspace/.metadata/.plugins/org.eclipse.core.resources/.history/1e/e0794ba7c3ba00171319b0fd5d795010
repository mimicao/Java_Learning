package cn.laoliu_02;

import com.laoliu_02.Animal;
import com.laoliu_02.Cat;
import com.laoliu_02.Dog;

public class AnimalDemo {
	public static void main(String[] args) {
		//抽象类不能被实例化
		//Animal a = new Animal();
		
		Animal a = new Cat();
		a.eat();
		a.sleep();
		
		System.out.println("-----------------");
		
		Animal d = new Dog();
		d.eat();
		d.sleep();
		d.jump();
		
		
	}
}
