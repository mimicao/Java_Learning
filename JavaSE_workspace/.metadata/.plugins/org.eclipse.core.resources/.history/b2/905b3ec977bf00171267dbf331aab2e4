package cn.laoliu_03;

public class StringTest2 {
	public static void main(String[] args) {
		String s1 = "Hello123World";
		int bigCount = 0;
		int smallCount = 0;
		int numCount = 0;
		char ch;
		for (int i = 0; i < s1.length(); i++) {
			ch = s1.charAt(i);
			if(ch>='0'&&ch<='9'){
				numCount++;
			}else if(ch>='a'&&ch<='z'){
				smallCount++;
			}else if(ch>='A'&&ch<='Z'){
				bigCount++;
			}else{
				
			}
		}
	}
}
